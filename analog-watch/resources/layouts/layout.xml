<layout id="WatchFace">
    <drawable class="Background" />
    <drawable id="Complication1" class="Complicated.ComplicationDrawable">
        <param name="locX">dc.getWidth() * .25</param>
        <param name="locY">dc.getHeight() * .5</param>
    </drawable>

    <drawable id="Complication2" class="Complicated.ComplicationDrawable">
        <param name="locX">dc.getWidth() * .75 </param>
        <param name="locY">dc.getHeight() * .5</param>
    </drawable>

    <drawable id="Complication3" class="Complicated.ComplicationDrawable">
        <param name="locX">dc.getWidth() / 2 </param>
        <param name="locY">dc.getHeight() * 0</param>
    </drawable>

    <drawable id="Complication4" class="Complicated.ComplicationDrawable">
        <param name="locX">dc.getWidth() / 2 </param>
        <param name="locY">dc.getHeight() * .75</param>
    </drawable>
    <label id="TimeLabel" x="center" y="center" font="Graphics.FONT_NUMBER_MEDIUM" justification="Graphics.TEXT_JUSTIFY_CENTER" color="Graphics.COLOR_BLUE" />
</layout>